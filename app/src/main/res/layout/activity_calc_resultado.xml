	<TextView
		android:id="@+id/sbaNetoRes"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintEnd_toEndOf="@+id/view2"
		app:layout_constraintStart_toEndOf="@+id/sbaNeto"
		tools:layout_editor_absoluteY="103dp" />

	<TextView
		android:id="@+id/sbaIrpfRes"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@+id/sbaIrpf"
		tools:layout_editor_absoluteY="159dp" />

	<TextView
		android:id="@+id/sbaDeduccionesRes"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@+id/sbaDeducciones"
		tools:layout_editor_absoluteY="225dp" />
</androidx.constraintlayout.widget.ConstraintLayout>